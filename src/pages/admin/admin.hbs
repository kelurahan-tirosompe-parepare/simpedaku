<div class="container mt-4 table-responsive">
<table class="table table-striped table-hover">
   <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Tanggal</th>
      <th scope="col">NIK</th>
      <th scope="col">Jenis Layanan</th>
      <th scope="col" class="text-center">Ket. RT/RW</th>
      <th scope="col" class="text-center">Status</th>
      <th scope="col">Keterangan</th>
      <th scope="col">Aksi</th>
    </tr>
  </thead>
  <tbody id="bodyRiwayat">
    {{#each riwayat}}
    <tr>
      <th class="align-middle" scope="row">{{noUrut @index}}</th>
      <td class="align-middle">{{this.[0]}}</td>
      <td class="align-middle">{{this.[2]}}</td>
      <td class="align-middle">{{this.[3]}}</td>
      <td class="align-middle text-center">
       {{this.[4]}}
      </td>
      <td class="align-middle text-center">
       {{this.[5]}}
      </td>
      <td>
        
        {{#link this.[6]}}
          {{url}}
        {{/link}}
        
      </td>
      <td class="align-middle text-center">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
        <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
      </svg>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
  <nav aria-label="Table navigation riwayat">
    <ul class="pagination justify-content-end">
