<div class="container mt-4 table-responsive">
<table class="table table-striped table-hover">
   <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Tanggal</th>
      <th scope="col">NIK</th>
      <th scope="col">Jenis Layanan</th>
      <th scope="col" class="text-center">Ket. RT/RW</th>
      <th scope="col" class="text-center">Status</th>
      <th scope="col">Keterangan</th>
      <th scope="col">Validasi</th>
    </tr>
  </thead>
  <tbody id="bodyRiwayat">
    {{#each riwayat}}
    <tr>
      <th class="align-middle" scope="row">{{noUrut @index}}</th>
      <td class="align-middle">{{this.[0]}}</td>
      <td class="align-middle">{{this.[2]}}</td>
      <td class="align-middle">{{this.[3]}}</td>
      <td class="align-middle text-center">
       {{this.[4]}}
      </td>
      <td class="align-middle text-center">
       {{this.[5]}}
      </td>
      <td>
        
    
        
        <div class="pswp-gallery pswp-gallery--single-column" id="gallery--berkas">
              {{#link this.[6]}}
                 {{ok}}
              {{/link}}
        </div>
        

        
      </td>
      <td class="align-middle text-center">
       <div class="form-check">
          <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
        </div>  
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
  <nav aria-label="Table navigation riwayat">
    <ul class="pagination justify-content-end">
      <li class="page-item disabled">
        <a class="page-link">&laquo;</a>
      </li>
      <li class="page-item"><a class="page-link" href="#">1</a></li>
      <li class="page-item"><a class="page-link" href="#">2</a></li>
      <li class="page-item"><a class="page-link" href="#">3</a></li>
      <li class="page-item">
        <a class="page-link" href="#">&raquo;</a>
      </li>
    </ul>
  </nav>
</div>


