<script type="module">
// Include Lightbox 
import PhotoSwipeLightbox from './phsw/photoswipe-lightbox.esm.js';
const options = { 
  dataSource: [
    {
      src: 'https://source.unsplash.com/Volo9FYUAzU/1620x1080',
      width: 3000,
      height: 3000,
      alt: 'test image 1'
    },

    {
      src: 'https://source.unsplash.com/RJzHlbKf6eY/1950x1300',
      width: 3000,
      height: 3000,
      alt: 'test image 2'
    },
  ],
  showHideAnimationType: 'zoom',
  // may select multiple "galleries"
  // gallery: '#link-open-phsw',

  // Elements within gallery (slides)
  children: 'a',

  // setup PhotoSwipe Core dynamic import
  pswpModule: () => import('./phsw/photoswipe.esm.js')
}

const lightbox = new PhotoSwipeLightbox(options);
lightbox.init();

$('#link-open-phsw').click(function(e){
  e.preventDefault()
  console.log(this)
  // console.log($('a').parents(e.target).child('a').filter())
  lightbox.loadAndOpen(); // defines start slide index
});

</script>